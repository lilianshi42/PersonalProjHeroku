<div class="row">
    <div class="col-md-12">
        <h2>Images</h2>
        <hr />
    </div>
    {{#each images}}
    <div class="col-md-4">
        <img src="/public/images/uploaded/{{this}}" class="img-responsive img-thumbnail" alt="Responsive image"/>
    </div>
    {{/each}}
    {{#unless images}}
    <div class="col-md-12 text-center">
        <strong>No Images Available</strong>
    </div>
    {{/unless}}
</div>